{
  "name": "fetchenv-ts",
  "version": "2.0.0",
  "description": "TypeScript-friendly environment variable getter",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "tsc",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ripixel/fetchEnv.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "ripixel@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ripixel/fetchEnv/issues"
  },
  "homepage": "https://github.com/ripixel/fetchEnv#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "@types/node": "^15.0.2",
    "semantic-release": "^17.4.2",
    "typescript": "^4.2.4"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}
